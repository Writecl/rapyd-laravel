{
    "name": "writecl/rapyd",
    "description": "crud widgets for laravel, to make an admin in few lines of code",
    "homepage": "https://github.com/writecl/rapyd-laravel",
    "keywords": ["laravel", "crud", "rapyd", "administrator", "admin", "backend", "datatable", "form"],
    "authors": [
        {
            "name": "Ricardo Valenzuela",
            "email": "rvalenzuelaferrada@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "^5|^6|^7",
        "laravelcollective/html": "^6.1",
        "intervention/image": "2.4.2",
        "writecl/burp": "^4"
    },
    "suggest": {
        "baum/baum": "Allows to use the DataTree widget"
    },
    "autoload": {
        "classmap": [
            "migrations"
        ],
        "psr-4": {
            "Writecl\\Rapyd\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Writecl\\Rapyd\\RapydServiceProvider"
            ]
        }
    },
    "require-dev": {
        "laravel/framework": "^5|^6|^7",
        "orchestra/testbench": "3.6.*",
        "phpunit/phpunit": "^5|^6|^7",
        "php-coveralls/php-coveralls": "^1.0"
    }
}
